        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:clickable="true"
        app:layout_constraintStart_toEndOf="@+id/chkNuggets"
        app:layout_constraintTop_toBottomOf="@+id/btnPlusAlitas"
        app:srcCompat="@drawable/foto_plus_foreground" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnPlusPatatas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="53dp"
        android:layout_marginTop="207dp"
        android:clickable="true"
        app:layout_constraintStart_toEndOf="@+id/chkPatatas"
        app:layout_constraintTop_toBottomOf="@+id/btnBack"
