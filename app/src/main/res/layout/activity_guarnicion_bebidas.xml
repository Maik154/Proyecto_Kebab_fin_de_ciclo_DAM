        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/btnPlusPatatas"
        app:layout_constraintTop_toBottomOf="@+id/btnBack" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnMinusPatatas3"
